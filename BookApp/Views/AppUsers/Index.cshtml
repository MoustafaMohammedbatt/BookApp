@model IEnumerable<Shared.DTOs.AppUserDTO>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Address</th>
            <th>Email</th>
            <th>Is Deleted</th>
            <th>Created On</th>
            <th>Admin Accepted</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Address</td>
                <td>@user.Email</td>
                <td>@user.IsDeleted</td>
                <td>@user.CreatedOn</td>
                <td>@user.AdminAccepted</td>
            </tr>
        }
    </tbody>
</table>
